{
  "name": "nuxt-rive",
  "version": "1.2.2",
  "description": "Nuxt Rive module",
  "keywords": [
    "nuxt",
    "module",
    "rive",
    "animation",
    "webgl",
    "nuxt-module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrnasil/nuxt-rive.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev docs",
    "dev:build": "nuxi build docs",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare docs",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "@rive-app/webgl": "^2.33.1",
    "@vueuse/core": "^14.1.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/devtools-ui-kit": "^3.1.1",
    "@nuxt/eslint": "^1.12.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "@release-it/conventional-changelog": "^10.0.4",
    "@types/node": "^25.0.3",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "release-it": "19.1.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  }
}
